/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@fboservices/*" :["src/app/services/*"],
      "@fboenvironments/*" :["src/environments/*"],
      "@fboutil/*" :["src/app/util/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
